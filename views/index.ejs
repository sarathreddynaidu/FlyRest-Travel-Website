                          class="tab-pane active"
                          id="flights"
                        >
                          <div class="row">
                            <div class="col-xxs-12 col-xs-6 mt">
                              <div class="input-field">
                                <label for="from">From:</label>
                                <input
                                  type="text"
                                  class="form-control"
                                  id="from-place"
                                />
                              </div>
                            </div>
                            <div class="col-xxs-12 col-xs-6 mt">
                              <div class="input-field">
                                <label for="from">To:</label>
